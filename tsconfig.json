{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "esnext",
    "declaration": true,
    "declarationDir": "types",
    "skipLibCheck": true,
    "sourceMap": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "lib": ["esnext", "dom"],
    "types": ["jest", "jest-playwright-preset", "expect-playwright"],
    "jsx": "react"
  },
  "exclude": [
    "dist",
    "example"
  ]
}
